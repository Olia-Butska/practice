#include <stdio.h>

// Функція для перевірки, чи є три сусідні цифри однаковими
int are_three_digits_equal(char s[], int i) {
  return (s[i] == s[i + 1] && s[i + 1] == s[i + 2]);
}

// Функція для підрахунку допустимих чисел
int count_valid_numbers(int p) {
  char allowed_digits[] = {'5', '9'};
  int dp[p + 1][2]; 

  // Базові випадки
  dp[1][0] = 1;
  dp[1][1] = 1;

  // Обчислення кількості допустимих чисел
  for (int i = 2; i <= p; i++) {
    for (int j = 0; j < 2; j++) {
      dp[i][j] = 0;

      // Додавання допустимих варіантів
      if (j == 0) {
        dp[i][j] += dp[i - 1][1]; // Додати числа, які закінчуються на 9
      } else {
        dp[i][j] += dp[i - 1][0]; // Додати числа, які закінчуються на 5
      }

      // Перевірка на наявність трьох однакових цифр поруч
      if (i >= 3 && !are_three_digits_equal(allowed_digits, j)) {
        dp[i][j] += dp[i - 2][j]; // Додати числа, які закінчуються на два різних символи
      }
    }
  }

  // Повернення кількості допустимих чисел з p розрядів
  return dp[p][0] + dp[p][1];
}

int main() {
  int p;
  scanf("%d", &p);

  // Перевірка правильності вхідних даних
  if (p < 1 || p > 30) {
    printf("Невірне значення число p має бути в діапазоні від 1 до 30.\n");
    return 1;
  }

  int valid_numbers = count_valid_numbers(p);

  // Виведення результату
  printf("%d\n", valid_numbers);

  return 0;
}
